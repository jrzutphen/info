{
	"lineWidth": 80,
	"useTabs": true,
	"indentWidth": 4,
	"newLineKind": "lf",
	"json": {
		"associations": [
			".all-contributorsrc",
			"devenv.lock"
		]
	},
	"markdown": {},
	"malva": {
		"hexColorLength": "long"
	},
	"yaml": {
		"useTabs": false,
		"indentWidth": 2
	},
	"plugins": [
		"https://plugins.dprint.dev/json-0.19.3.wasm",
		"https://plugins.dprint.dev/markdown-0.17.2.wasm",
		"https://plugins.dprint.dev/g-plane/malva-v0.10.1.wasm",
		"https://plugins.dprint.dev/g-plane/pretty_yaml-v0.4.0.wasm"
	]
}
